
public class Main
{
	public static void main(String[] args) {
	    Scanner sc =new Scanner(System.in);
		System.out.println("Enter no of hai:");
		int x=sc.nextInt();
		System.out.println("Enter no of rows:");
		int y=sc.nextInt();
		System.out.println("Enter no of blocks:");
		int n=sc.nextInt();
		int count=0;
		for(int k=0;k<n;k++){
		for(int i=0;i<x;i++){
		    for(int j=0;j<y;j++){
		    System.out.print("Hai"+" ");
		    count++;
		}
		System.out.println();
	}
}
}
}
