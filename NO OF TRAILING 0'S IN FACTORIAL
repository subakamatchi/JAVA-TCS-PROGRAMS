import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc =new Scanner(System.in);
	    System.out.println("Enetr the no:");
	    int num=sc.nextInt();
	    int count=0;
	    for(int i=5;num/i>0;i=i+5){
	        count=count+num/i;
	    }
	    System.out.println(count);
		
	}
}
