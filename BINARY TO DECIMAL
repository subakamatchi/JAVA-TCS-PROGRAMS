import java.util.*;

public class Main
{
	public static void main(String[] args) {
	   String str="1010";
	   int d=0;
	   int pow=0;
	   for(int i=str.length()-1;i>=0;i--){
	     char bit =str.charAt(i);
	     if(bit=='1'){
	         d+=Math.pow(2,pow);
	     }
	     pow++;
	   }
	   System.out.println(d);
	   }
		
	}
